syntax: regexp

#Everything in Config but AU files
^Config/(?!Earth|Vessels).*$
^Config/Earth/(?!Base).*$
^Config/Earth/Base/(?!Wideawake).*$
^Config/Vessels/(?!Ascension).*$

#Everything in Doc but Ascension folder
^Doc/(?!Ascension).*$

#Everything in Meshes but Ascension and CSSC folder
^Meshes/(?!Ascension|cssc).*$

#Everything in Modules but AU files
^Modules/(?!Plugin|Startup|Ascension|VirtualDocking).*$
^Modules/Plugin/(?!Ascension).*$
^Modules/Startup/(?!Ascension).*$

#Prepare for optional files here under Ascension prefix
^Flights/(?!Ascension).*$
^Images/(?!Ascension).*$
^Html/(?!Ascension).*$
^Textures2/(?!Ascension).*$

#Everything in Orbitersdk but Ascension code
^Orbitersdk/(?!samples).*$
^Orbitersdk/samples/(?!Ascension).*$

#Prepare for optional files here with AU, Ascension or Wideawake prefix
^Scenarios/(?!AU|Ascension|Wideawake).*$
^Script/(?!AU|Ascension|Wideawake).*$
^Sound/(?!AU|Ascension|Wideawake).*$

#Everything in Textures but Ascension and CSSC folder
^Textures/(?!Ascension|cssc).*$


